{
    "main": "./src/index.ts",
    "devDependencies": {
        "@types/cors": "2.8.5",
        "@types/express": "4.17.0",
        "@types/express-session": "1.15.13",
        "@types/jest": "22.2.3",
        "@types/node": "12.0.10",
        "@types/npmlog": "4.1.2",
        "@types/yargs": "13.0.0",
        "jest": "24.8.0",
        "json-schema-to-typescript": "6.1.3",
        "prettier": "1.18.2",
        "source-map-support": "0.5.12",
        "tslint": "5.18.0",
        "tslint-config-prettier": "1.18.0",
        "typescript": "3.5.2"
    },
    "dependencies": {
        "cors": "2.8.5",
        "express": "4.17.1",
        "express-session": "1.16.2",
        "npmlog": "4.1.2",
        "yargs": "13.2.4"
    },
    "config": {
        "java_tracer": "cd ../tracers/java/ && . ./package.sh start",
        "python_tracer": "cd ../tracers/python/ && . ./package.sh start"
    },
    "scripts": {
        "format": "shopt -s extglob globstar && tslint -c ./tslint.json -p ./package.json && prettier --config ./.prettierrc.js -c --write ./src/**/*.ts* && shopt -u extglob globstar",
        "schema": "mkdir -p ./src/schema && json2ts -i ../schemas/schema.json -o ./src/schema/schema.ts",
        "debug": "tsc && node -r source-map-support/register --nolazy --inspect-brk -- ./dist/src/index.js --tracer java \"$npm_package_config_java_tracer\" --tracer python \"$npm_package_config_python_tracer\"",
        "start": "tsc && node -r source-map-support/register -- ./dist/src/index.js --tracer java \"$npm_package_config_java_tracer\" --tracer python \"$npm_package_config_python_tracer\"",
        "test": "tsc && TRACER_JAVA=\"$npm_package_config_java_tracer#public class Main{public static void main(String[] a){}}#error+-\" TRACER_PYTHON=\"$npm_package_config_python_tracer##error+-\" jest --testRegex 'dist/test/.*\\.test\\.js' --"
    }
}
