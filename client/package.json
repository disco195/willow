{
    "main": "./public/index.html",
    "devDependencies": {
        "@types/classnames": "*",
        "@types/node": "*",
        "@types/react": "*",
        "@types/react-dom": "*",
        "@types/react-grid-layout": "*",
        "@types/redux-thunk": "*",
        "parcel-bundler": "*",
        "tslint": "*",
        "typescript": "*"
    },
    "dependencies": {
        "axios": "*",
        "brace": "*",
        "classnames": "*",
        "emotion": "*",
        "react": "*",
        "react-dom": "*",
        "react-grid-layout": "*",
        "redux": "*",
        "redux-thunk": "*"
    },
    "config": {
        "server": "http://localhost:8001"
    },
    "scripts": {
        "lint": "tslint --config ./tslint.json --project ./package.json",
        "build": "parcel build --no-cache --no-source-maps --public-url ./ --out-dir ./dist/build -- $npm_package_main",
        "start": "SERVER_ADDRESS=$npm_package_config_server parcel serve --no-cache --out-dir ./dist/ --port 8000 -- $npm_package_main"
    }
}
