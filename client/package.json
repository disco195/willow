{
    "main": "./public/index.html",
    "devDependencies": {
        "@types/react": "16.9.34",
        "@types/react-dom": "16.9.6",
        "parcel-bundler": "latest",
        "prettier": "latest",
        "typescript": "latest"
    },
    "dependencies": {
        "@babel/polyfill": "7.8.7",
        "axios": "0.19.2",
        "brace": "0.11.1",
        "emotion": "10.0.27",
        "firebase": "7.14.4",
        "react": "16.13.1",
        "react-contexify": "4.1.1",
        "react-dom": "16.13.1"
    },
    "scripts": {
        "format": "shopt -s extglob globstar && prettier --config ./.prettierrc.json -c --write ./src/**/*.ts* && shopt -u extglob globstar",
        "build": "parcel build --no-cache --no-source-maps -d ./dist/build -- \"$npm_package_main\"",
        "start": "parcel serve --no-cache -d ./dist/ -p 5000 -- \"$npm_package_main\"",
        "serve": "npm run build && cd ../ && npx firebase emulators:start --only hosting",
        "deploy": "npm run build && cd ../ && npx firebase deploy --only hosting"
    }
}
